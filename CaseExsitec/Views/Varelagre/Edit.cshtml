@model CaseExsitec.Models.Varelagre

@{
    ViewData["Title"] = "Edit";
}

<h1>Rediger</h1>

<h4>Varelagre</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Lagernr" class="control-label"></label>
                <input asp-for="Lagernr" class="form-control" />
                <span asp-validation-for="Lagernr" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Sted" class="control-label"></label>
                <input asp-for="Sted" class="form-control" />
                <span asp-validation-for="Sted" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input style="margin-top: 10px;" type="submit" value="Lagre" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a style="margin-top: 10px; padding-left: 7px;" asp-action="Index">Tilbake</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
